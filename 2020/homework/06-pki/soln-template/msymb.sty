%% particular structures

%% Sets

\if@blackboardsets
  \newcommand{\naturals}{\ensuremath{\mathbb{N}}\xspace}
  %% the integer ring modulo #1 (Galois field if #1 prime power)
  \newcommand{\integers}[1][]{\ensuremath{\mathbb{Z}_{#1}}\xspace}
  \newcommand{\rationals}{\ensuremath{\mathbb{Q}}\xspace}
  \newcommand{\reals}{\ensuremath{\mathbb{R}}\xspace}
  \newcommand{\complex}{\ensuremath{\mathbb{C}}\xspace}
  \newcommand{\bools}{\ensuremath{\mathbb{B}}\xspace}
\else
  \newcommand{\naturals}{\ensuremath{\mathbf{N}}\xspace}
  %% the integer ring modulo #1 (Galois field if #1 prime power)
  \newcommand{\integers}[1][]{\ensuremath{\mathbf{Z}_{#1}}\xspace}
  \newcommand{\rationals}{\ensuremath{\mathbf{Q}}\xspace}
  \newcommand{\reals}{\ensuremath{\mathbf{R}}\xspace}
  \newcommand{\complex}{\ensuremath{\mathbf{C}}\xspace}
  \newcommand{\bools}{\ensuremath{\mathbf{B}}\xspace}
\fi
%% non-negative reals
\newcommand{\nnreals}{\ensuremath{\reals_{\geq0}}\xspace}
%% positive reals
\newcommand{\preals}{\ensuremath{\reals_{>0}}\xspace}
%% negative reals
\newcommand{\nreals}{\ensuremath{\reals_{<0}}\xspace}
%% non-positive reals
\newcommand{\npreals}{\ensuremath{\reals_{\leq0}}\xspace}
%% algebraic numbers
\newcommand{\algebraics}{\ensuremath{\bar{\rationals}}\xspace}

%%

% characteristic function / indicator function of the set #1
\newcommand{\charf}[1]{I_{#1}}%{I_{#1}}%{\chi_{#1}}%{1_{#1}}

%% mathematical language
%% set selection
\def\withsym{\mathrel{\mathop{:}}}
\def\with{~\withsym~}
%% the powerset of a set, i.e. the set of all subsets.
\newcommand{\powerset}[1]{\wp(#1)}
%% multiset or bag containing #1
\newcommand{\bag}[1]{\{\kern-0.4em\{#1\}\kern-0.4em\}}
%% the cardinality i.e. number of elements of a set
\newcommand{\cardinality}[1]{|#1|}

%% \related{R}{x}{y} says that the relation R holds between x and y (set notation)
\newcommand{\relateds}[3]{(#2,#3)\in#1}
%% \related{R}{x}{y} says that the relation R holds between x and y (inline notation)
\newcommand{\relatedi}[3]{#2#1#3}
\if@inlinerelation
  \let\related\relatedi
\else
  \let\related\relateds
\fi
%% \unrelated{R}{x}{y} says that the relation R does not hold between x and y (set notation)
\newcommand{\unrelated}[3]{(#2,#3)\not\in#1}

%% Two groups/logics/whatever are isomorphic
\newcommand{\isomorphic}{\equiv}

% isomorphism \to
\newcommand{\isoto}{\xrightarrow{\smash{\raisebox{-0.65ex}{\ensuremath{\scriptstyle\sim}}}}}
   
%% the absolute value of #1
\newcommand*{\abs}[1]{\lvert#1\rvert}%{|#1|}

% Gauss brackets for floor and ceiling
\newcommand*{\floor}[1]{\lfloor#1\rfloor}
\newcommand*{\ceil}[1]{\lceil#1\rceil}

%% the placeholder for arbitrary arguments for composites
%% @see lsyntax.sty#\margholder
\newcommand{\argholder}{\cdot}
%% argument place marker
\let\argplace\argholder

%% emphasize in math mode
\providecommand*{\memph}[1]{\boldsymbol{#1}}

%% convex hull
\DeclareMathOperator{\convexhull}{Conv}
%%NOTE: equivalent to \newcommand{\convexhull}{\operatorname{Conv}}

%% inverse image / preimage of a function
\newcommand*{\ipreimage}[1]{{#1}^{-1}}

%% set complement
\newcommand*{\scomplement}[1]{#1^\complement}%


%% Laplace operator for partial differential equations
\newcommand{\laplace}{\Delta}

%%\superdiff{u} the Fr\'echet superdifferential of u
\newcommand*{\superdiff}[1]{\mathord{D^+}#1}%
%%\subdiff{u} the Fr\'echet subdifferential of u
\newcommand*{\subdiff}[1]{\mathord{D^-}#1}%



