% Springer books: 
\newcommand{\emath}{\mathrm{e}}
%% imaginary unit i
% Springer books: \newcommand{\imag}{{\rm i}}
% the real number \pi
\newcommand{\pimath}{\pi}


%% the invisible times operator
\newcommand{\itimes}{}
%% the scalar product times operator
\newcommand{\stimes}{\mathbin{\boldsymbol{\cdot}}}%{\mathbin{\cdot}}%{\mathbin{\bullet}}%{\mathbin{\text{\Large$\cdot$}}}

%% defined to be equal
\def\eqdef{\mathrel{\mathop{:}}=}
%% obsolete
\def\defeq{~\mathrel{\mathop{:}}=~}%%{~\stackrel{def}{\mathrel{\mathop{:}}=}~}
%% defined to be equivalent
\def\defequiv{\mathrel{\mathop{:}}\Longleftrightarrow}

