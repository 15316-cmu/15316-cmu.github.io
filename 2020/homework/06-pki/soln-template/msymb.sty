%% smaller version of \dots
\newcommand{\sdots}{\mathinner{\ldotp\ldotp}}%
%% smaller version of minus -
\newcommand{\sminus}{{\scriptsize-}}%

%%circled symbols \textcircled{$\star$} from amsmath

%% Superimpose two symbols. The wider symbol must go first in the argument to \superimpose, which must consist of two braced objects.
%% @see \rlap, \llap, \clap to print symbols with zero-width box
%% @see \usepackage{mathtools} \mathrlap{\vee}\wedge
%% @see ${\mathrel{\mathpalette\superimpose{{\vee}{\wedge}}}}$
\newcommand*{\superimpose}[2]%
{{\ooalign{$#1\@firstoftwo#2$\cr\hfil$#1\@secondoftwo#2$\hfil\cr}}}


% math squeeze, squeeze symbols tighter together in math mode like {+} would
\newcommand*{\msqueeze}[1]{{\setlength{\medmuskip}{0mu}#1}}
\newcommand*{\msqueezee}[1]{{\setlength{\thinmuskip}{0mu}\setlength{\medmuskip}{0mu}\setlength{\thickmuskip}{0mu}#1}}
%\medmuskip is used around binary operators (like \times) and \thickmuskip is used around binary relations (like =)

%% misc funny brackets
%% \[
%% \lbanana a \rbanana \qquad
%% \lenvelope a \renvelope \qquad
%% \llense a \rlense \qquad
%% \llensehat a \rlensehat \qquad
%% \lelbow a \relbow \qquad
%% \Langle a \Rangle \qquad
%% \lbarbedwire a \rbarbedwire \qquad
%% \llid a \rlid \qquad
%% \lstrike a \rstrike \qquad
%% \lsmooch a \rsmooch \qquad
%% \]
% (| 
\newcommand{\lbanana}{\mathopen{\mathrel{(}\joinrel\mathrel{|}}}
% |)
\newcommand{\rbanana}{\mathclose{\mathrel{|}\joinrel\mathrel{)}}}
% [|
% @see also \usepackage{stmaryrd} \llbracket
\newcommand{\lenvelope}{\mathopen{\mathrel{[}\joinrel\mathrel{[}}}
% |]
\newcommand{\renvelope}{\mathclose{\mathrel{]}\joinrel\mathrel{]}}}
% [(
%%\todo improve notation
\newcommand{\llense}{\mathopen{\mathrel{[}\joinrel\mathrel{(}}}
% )]
\newcommand{\rlense}{\mathclose{\mathrel{)}\joinrel\mathrel{]}}}
% [<
%%\todo improve notation
\newcommand{\llensehat}{\mathopen{\mathrel{[}\joinrel\mathrel{\langle}}}
% >]
\newcommand{\rlensehat}{\mathclose{\mathrel{\rangle}\joinrel\mathrel{]}}}
% << as in stereotype
\newcommand{\lelbow}{\mathopen{\ll}}
% >>
\newcommand{\relbow}{\mathclose{\gg}}
% << bigger with \langle
%%\todo improve notation
\newcommand{\Langle}{\mathopen{\mathrel{\langle}\joinrel\mathrel{\langle}}}
% >> bigger with \rangle
\newcommand{\Rangle}{\mathclose{\mathrel{\rangle}\joinrel\mathrel{\rangle}}}
% {| tight
\newcommand{\lbarbedwire}{{\{}\!\!{|}}
% |} tight
\newcommand{\rbarbedwire}{{|}\!\!{\}}}
% {| spacious
\newcommand{\llid}{\mathopen{\mathrel{\{}\joinrel\mathrel{|}}}
% |} spacious
\newcommand{\rlid}{\mathclose{\mathrel{|}\joinrel\mathrel{\}}}}
% <[
\newcommand{\lstrike}{\mathopen{\mathrel{\langle}\joinrel\mathrel{[}}}
% ]>
\newcommand{\rstrike}{\mathclose{\mathrel{]}\joinrel\mathrel{\rangle}}}
% <-<
\newcommand{\lsmooch}{\mathopen{{<}\!\!\!\!{\prec}}}
% >->
\newcommand{\rsmooch}{\mathclose{{\succ}\!\!\!\!{>}}}



% \lightning of \usepackage{wasysym} or \lightning of \usepackage{stmaryrd} or \usepackage{MnSymbol} or \usepackage{marvosym}
\newcommand{\contradiction}{~\textrm{CONTRADICTION!}}

%% base of natural logarithm resp. exponential
